{
  "$schema": "https://swc.rs/schema.json",
  "module": {
    "type": "nodenext",
    "resolveFully": true
  },
  "sourceMaps": true,
  "jsc": {
    "parser": {
      "syntax": "typescript",
    },
    "experimental": {
      "keepImportAssertions": true
    },
    "baseUrl": "./",
    "paths": {
      "src/*": [
        "./src/*"
      ],
      "@/*": [
        "./src/*"
      ]
    }
  }
}
